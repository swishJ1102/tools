def simulate_hierarchy_from_excel(excel_files):
    hierarchy = {}  # 用字典来模拟文件夹层级关系
    
    for excel_file in excel_files:
        # 这里可以添加读取Excel文件并提取文件名的代码，这里简化为直接用文件名来模拟层级
        file_name = excel_file.split('.')[0]  # 假设Excel文件名就是文件夹名
        
        current_level = hierarchy
        path = ""
        for char in file_name:
            path += char
            if path not in current_level:
                current_level[path] = {}
            current_level = current_level[path]
    
    # 递归遍历字典并输出层级关系
    def print_hierarchy(level, indent=""):
        for key in sorted(level.keys()):
            print(indent + key)
            print_hierarchy(level[key], indent + "  ")
    
    print_hierarchy(hierarchy)

# 假设excel_files是遍历到的Excel文件列表
excel_files = ["a.xlsx", "b.xlsx", "c.xlsx", "g.xlsx"]

# 输出文件名的层级关系
simulate_hierarchy_from_excel(excel_files)